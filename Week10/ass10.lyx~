omega = np.random.uniform(0.5,1.5)#random
\end_layout

\begin_layout Plain Layout

delta = np.random.uniform(-1,1)*pi #random 
\end_layout

\begin_layout Plain Layout

print omega 
\end_layout

\begin_layout Plain Layout

print delta 
\end_layout

\begin_layout Plain Layout

t = linspace(-4*pi,4*pi,257) 
