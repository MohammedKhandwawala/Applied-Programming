\end_layout

\begin_layout Plain Layout

Y = fftshift(fft(y))/512.0
\end_layout

\begin_layout Plain Layout

w=linspace(-pi*fmax,pi*fmax,257)
\end_layout

\begin_layout Plain Layout

w=w[:-1]
\end_layout

\end_inset


\end_layout

