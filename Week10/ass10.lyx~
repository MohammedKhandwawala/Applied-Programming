n=arange(256) 
\end_layout

\begin_layout Plain Layout

wnd= fftshift(0.54+0.46*cos(2*pi*n/256)) 
\end_layout

\begin_layout Plain Layout

y = cos( omega * t + delta ) 
\end_layout

\begin_layout Plain Layout

y[0] = 0 
\end_layout

\begin_layout Plain Layout

